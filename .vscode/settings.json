{
  "css.customData": ["./vscode.tailwind.json"],
  "eslint.validate": ["javascript", "javascriptreact", "astro", "typescript", "typescriptreact"],
  "files.associations": {
    "*.mdx": "markdown"
  },
  "prettier.documentSelectors": ["**/*.astro"],
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "yaml.schemas": {
    "./.vscode/astrowind/config-schema.json": "/src/config.yaml"
  },
  "eslint.experimental.useFlatConfig": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#29bf77",
    "activityBar.background": "#29bf77",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#9247d8",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#29bf77",
    "statusBar.background": "#20955d",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#29bf77",
    "statusBarItem.remoteBackground": "#20955d",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#20955d",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#20955d99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#20955d"
}
